{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "introspect": " drizzle-kit introspect --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:migrate": "DB_MIGRATING=true ts-run ./server/database/tasks/migrate.ts",
    "db:seed": "DB_SEEDING=true ts-run ./server/database/tasks/seed.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "1.2.0",
    "@nuxt/fonts": "0.11.0",
    "@nuxt/icon": "1.11.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.0.1",
    "@nuxthub/core": "0.8.21",
    "@pinia/nuxt": "0.10.1",
    "drizzle-orm": "^0.41.0",
    "eslint": "^9.0.0",
    "nuxt": "^3.16.1",
    "pinia": "^3.0.1",
    "postgres": "^3.4.5",
    "typescript": "^5.6.3",
    "typescript-run": "^0.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.32",
    "@types/node": "^22.13.13",
    "drizzle-kit": "^0.30.5",
    "nuxt-lodash": "^2.5.3",
    "wrangler": "^4.5.1"
  }
}
